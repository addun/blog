{
  "extends": "astro/tsconfigs/strict",
  "compilerOptions": {
    "typeRoots": ["./astro/types.d.ts"],
    "baseUrl": ".",
    "paths": {
      "@components/*": ["src/components/*"],
      "@icons/*": ["src/components/icons/*"],
      "@utils/*": ["src/utils/*"],
      "@layouts/*": ["src/layouts/*"],
      "@demos/react/*": ["src/demos/react/*"],
      "@integrations/*": ["integrations/*"]
    }
  }
}
